    <fields>
        <fullName>Total_Correct__c</fullName>
        <description>Adds weights and adds all yes answers</description>
        <externalId>false</externalId>
        <formula>CASE( Clearly_Out__c, &quot;Yes&quot;, 10, &quot;No&quot;, 0, &quot;N/A&quot;, 0,0) + 
CASE( Client_confirmed_ok_to_close__c, &quot;Yes&quot;, 5, &quot;No&quot;, 0, &quot;N/A&quot;, 0,0) + 
CASE( Consistent_timely_updates_follow_up__c , &quot;Yes&quot;, 10, &quot;No&quot;, 0, &quot;N/A&quot;, 0,0) + 
CASE( Contains_Searchable_Terms_Keywords__c , &quot;Yes&quot;, 10, &quot;No&quot;, 0, &quot;N/A&quot;, 0,0) + 
CASE( Milestones_accurately_captured_noted__c , &quot;Yes&quot;, 10, &quot;No&quot;, 0, &quot;N/A&quot;, 0,0) + 
CASE( Outline_of_steps_to_get_given_error__c , &quot;Yes&quot;, 7.5, &quot;No&quot;, 0, &quot;N/A&quot;, 0,0) + 
CASE( Proper_article_associated__c , &quot;Yes&quot;, 25, &quot;No&quot;, 0, &quot;N/A&quot;, 0,0) + 
CASE( Screenshots__c , &quot;Yes&quot;, 5, &quot;No&quot;, 0, &quot;N/A&quot;, 0,0) + 
CASE( State_expected_vs_actual_results__c , &quot;Yes&quot;, 7.5, &quot;No&quot;, 0, &quot;N/A&quot;, 0,0) + 
CASE( Updates_are_provided_to_clients__c , &quot;Yes&quot;, 10, &quot;No&quot;, 0, &quot;N/A&quot;, 0,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Adds weights and adds all yes answers</inlineHelpText>
        <label>Total Correct</label>
        <precision>18</precision>
        <required>false</required>
        <scale>1</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Total_Eligable__c</fullName>
        <description>Adds weights and counts Yes/No answers</description>
        <externalId>false</externalId>
        <formula>CASE( Clearly_Out__c, &quot;Yes&quot;, 10, &quot;No&quot;, 10, &quot;N/A&quot;, 0,0) +
CASE(  Client_confirmed_ok_to_close__c, &quot;Yes&quot;, 5, &quot;No&quot;, 5, &quot;N/A&quot;, 0,0) +
CASE(  Consistent_timely_updates_follow_up__c , &quot;Yes&quot;, 10, &quot;No&quot;, 10, &quot;N/A&quot;, 0,0) +
CASE(  Contains_Searchable_Terms_Keywords__c , &quot;Yes&quot;, 10, &quot;No&quot;, 10, &quot;N/A&quot;, 0,0) +
CASE(  Milestones_accurately_captured_noted__c , &quot;Yes&quot;, 10, &quot;No&quot;, 10, &quot;N/A&quot;, 0,0) +
CASE(  Outline_of_steps_to_get_given_error__c , &quot;Yes&quot;, 7.5, &quot;No&quot;, 7.5, &quot;N/A&quot;, 0,0) +
CASE(  Proper_article_associated__c , &quot;Yes&quot;, 25, &quot;No&quot;, 25, &quot;N/A&quot;, 0,0) +
CASE(  Screenshots__c , &quot;Yes&quot;, 5, &quot;No&quot;, 5, &quot;N/A&quot;, 0,0) + 
CASE(  State_expected_vs_actual_results__c , &quot;Yes&quot;, 7.5, &quot;No&quot;, 7.5, &quot;N/A&quot;, 0,0) +
CASE(  Updates_are_provided_to_clients__c , &quot;Yes&quot;, 10, &quot;No&quot;, 10, &quot;N/A&quot;, 0,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Adds weights and counts Yes/No answers</inlineHelpText>
