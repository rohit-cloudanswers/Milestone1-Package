    </fields>
    <fields>
        <fullName>Closed_DateTime__c</fullName>
        <description>The Date and Time that this status was changed to something else. The current status entry will have a null value for this field. This is used to calculate minutes-in-status.</description>
        <externalId>false</externalId>
        <label>Closed DateTime</label>
        <required>false</required>
        <trackTrending>true</trackTrending>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>IsStopped__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Indicates whether the SLA clock is stopped during this status or not.</description>
        <externalId>false</externalId>
        <inlineHelpText>Indicates whether the SLA clock is stopped during this status or not.</inlineHelpText>
        <label>IsStopped</label>
        <type>Checkbox</type>
    </fields>
    <fields>
