        <length>50</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Previous_Status__c</fullName>
        <externalId>false</externalId>
        <label>Previous Status</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>RawMinutesInStatus__c</fullName>
        <externalId>false</externalId>
        <formula>IF(IsStopped__c,
    0,
    IF(ISNULL(Closed_DateTime__c), 
        -1,
        (Closed_DateTime__c -  CreatedDate)*1440
      )  
  )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>RawMinutesInStatus</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
