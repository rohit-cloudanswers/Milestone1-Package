    <webLinks>
        <fullName>Submit</fullName>
        <availability>online</availability>
        <description>This button submits the bug to the bug systems</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/apex.js&quot;)}
var newRecords = []; 

var b = new sforce.SObject(&quot;Bug__c&quot;); 
var user = sforce.connection.getUserInfo(); 

b.id =&quot;{!Bug__c.Id}&quot;; 
b.Submitted_Date__c = new Date().toISOString(); 
b.Submitted_By__c = user.userId; 
var cont = &quot;true&quot;;
if(&quot;{!Bug__c.Submitted_Date__c}&quot;!=&quot;&quot;){
  cont = &quot;false&quot;;
  if(prompt(&quot;This bug has already been submitted.\nType &apos;y&apos; if you want to resubmit.&quot;,&apos;&apos;)==&quot;y&quot;){
    cont = &quot;true&quot;;
  }
}
if(cont==&quot;true&quot;){

newRecords.push(b); 
result = sforce.connection.update(newRecords); 


//check for errors 
var message = &quot;&quot;; 

if (result[0].getBoolean(&apos;success&apos;)) { 
  //success 
  response = sforce.apex.execute(&quot;blogic_Bug&quot;,&quot;sendBug&quot;,{bugId:b.id});
  if(response ==&quot;Success.&quot;){
    alert(&quot;The bug has been submitted successfully.&quot;);
    window.location.reload(); 
  } else {
    alert(response);
  }
  
} else { 
  for (var i=0; i&lt;result.length; i++) { 
    message += &quot;\n&quot;; 
    message += result[i].errors.message; 
  } 
  message = &quot;An error occurred:\n&quot; + message; 
  alert(message); 
}
}</url>
