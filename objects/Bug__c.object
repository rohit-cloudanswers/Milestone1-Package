        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)}

var newUrl = &apos;a1M/e?CF00Ni000000GHwjH={!Case.CaseNumber}&amp;CF00Ni000000GHwjH_lkid={!Case.Id}&amp;RecordType=012i0000000xw9l&amp;retURL=%2F{!Case.Id}&apos;;

 var caseQuery = sforce.connection.query(&quot;SELECT TargetDate, CaseId From CaseMilestone  WHERE CaseId = &apos;{!Case.Id}&apos; AND MilestoneType.Name = &apos;Resolution&apos; &quot;);

var caseMile = caseQuery.getArray(&quot;records&quot;);

if( caseMile.length &gt; 0){
  var caseDate =caseMile[0].TargetDate.split(&apos;T&apos;)[0];

  var dates = caseDate.split(&apos;-&apos;); //yyyy-month-day

  newUrl = newUrl + &apos;&amp;00Ni000000GHwka=&apos;+ dates[1]+&apos;/&apos;+dates[2]+&apos;/&apos;+dates[0];
}
location.replace(newUrl);</url>
