    <validationRules>
        <fullName>ValidationReasonForRed</fullName>
        <active>true</active>
        <errorConditionFormula>(OR(ISPICKVAL( Program__r.Delivery_Health__c , &apos;Red&apos;),
ISPICKVAL( Program__r.Budget_Health__c , &apos;Red&apos;),
ISPICKVAL( Program__r.Revenue_Rec_Health__c , &apos;Red&apos;),
ISPICKVAL( Program__r.Delivery_Health__c , &apos;Yellow&apos;),
ISPICKVAL( Program__r.Budget_Health__c , &apos;Yellow&apos;),
ISPICKVAL( Program__r.Revenue_Rec_Health__c , &apos;Yellow&apos;)))
&amp;&amp;
(ISBLANK( Reason_For_Red_Amber__c ))</errorConditionFormula>
        <errorMessage>Please enter &quot;Reason For Red/Amber&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValidationforPathtoGreen</fullName>
        <active>true</active>
        <errorConditionFormula>(OR(ISPICKVAL( Program__r.Delivery_Health__c , &apos;Red&apos;),
ISPICKVAL( Program__r.Budget_Health__c , &apos;Red&apos;),
ISPICKVAL( Program__r.Revenue_Rec_Health__c , &apos;Red&apos;),
ISPICKVAL( Program__r.Delivery_Health__c , &apos;Yellow&apos;),
ISPICKVAL( Program__r.Budget_Health__c , &apos;Yellow&apos;),
ISPICKVAL( Program__r.Revenue_Rec_Health__c , &apos;Yellow&apos;)))
&amp;&amp;
(ISBLANK( Path_to_Green__c ))</errorConditionFormula>
        <errorMessage>Please enter &quot;Path to Green&quot;.</errorMessage>
    </validationRules>
