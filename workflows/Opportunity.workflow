    <fieldUpdates>
        <fullName>OpportunityCloseDate</fullName>
        <description>workflow trigger to change the close date = Created date + 7</description>
        <field>CloseDate</field>
        <formula>CreatedDate + 7</formula>
        <name>OpportunityCloseDate</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>Opportunity_Stage_Field_Update</fullName>
        <description>Defaults the Opportunity Stage to &quot;01 - Prospect Not Qualified&quot;</description>
        <field>StageName</field>
        <literalValue>01 - Prospect - Not Qualified</literalValue>
        <name>Opportunity Stage Field Update</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>Requested_Date_Ship_35</fullName>
        <field>Customer_Request_Date_Ship__c</field>
        <formula>CloseDate + 35</formula>
        <name>Requested Date (Ship) +35</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
