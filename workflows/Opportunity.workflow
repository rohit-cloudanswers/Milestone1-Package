    <rules>
        <fullName>Adtech New Oppty to Full Edit</fullName>
        <actions>
            <name>Change_Adtech_Oppty_record_type</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Opportunity.RecordTypeId</field>
            <operation>equals</operation>
            <value>Adtech Sales - New</value>
        </criteriaItems>
        <description>Change Adtech Sales - new to Adtech Sales type after create</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Adtech Override Amount on NonSyncd</fullName>
        <actions>
            <name>Adtech_Amount_Override</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Opportunity.RecordTypeId</field>
            <operation>equals</operation>
            <value>Adtech Sales - New,Adtech Sales</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.Is_Synced__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <description>Override the amount field with the Total Value field on non-synced opptys</description>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>BA Submitted Email</fullName>
