            <name>BA_Submitted_Email</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>Email when Bid Approval is submitted</description>
        <formula>ISCHANGED(BA_Submit_Date__c)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Default Opportunity Stage</fullName>
        <actions>
            <name>Opportunity_Stage_Field_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Opportunity.CreatedDate</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <description>Defaults the opportunity stage upon creation</description>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Media 100%25 Opportunity</fullName>
        <actions>
            <name>Media_Opty_Hits_100_Email</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>Media Opportunity has moved to 100% (Closed)</description>
        <formula>(CONTAINS(TEXT( Line_of_Business__c) , &quot;Ad Tech&quot;) ||
	    CONTAINS(TEXT( Line_of_Business__c) , &quot;Media&quot;)) &amp;&amp;
PRIORVALUE( Probability ) = 0.90 &amp;&amp;
	 Probability = 1</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Opportunity Stage Track Rule</fullName>
        <actions>
            <name>UpdateOpportunity</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Opportunity.StageName</field>
            <operation>equals</operation>
            <value>Stage 3</value>
        </criteriaItems>
        <description>Keep track of Stage while opportunity updated.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
