    </rules>
    <rules>
        <fullName>P%26N Requested Shipdate Default if Blank</fullName>
        <actions>
            <name>Requested_Date_Ship_35</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Opportunity.Customer_Request_Date_Ship__c</field>
