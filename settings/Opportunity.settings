        <similarOpportunitiesMatchFields>OPPORTUNITY.ACCOUNT</similarOpportunitiesMatchFields>
        <similarOpportunitiesMatchFields>OPPORTUNITY.TYPE</similarOpportunitiesMatchFields>
        <similarOpportunitiesMatchFields>OPPORTUNITY.OWNER</similarOpportunitiesMatchFields>
