  <script type="text/javascript">
  
      /*This function hides/unhides the dropdown menu, the second parameter is used to change the arrow icon on the right hand side of the button*/
      function changeElement(id, parent) {
          var el = document.getElementById(id);
          
          /* CSS changes are normally made inline, this is a workaround so it needs to be clicked twice*/
          if ( el.style.visibility=="" ) {
              el.style.visibility="visible";
              
              try { parent.style.backgroundPosition="right 3px"; }
              catch(err) {}
          }
          
          else if ( el.style.visibility!="hidden") {
              el.style.visibility="hidden";
              
              try { parent.style.backgroundPosition="right -21px"; }
              catch(err) {}
          }
          else {
              el.style.visibility="visible";
              
              try { parent.style.backgroundPosition="right 3px"; }
              catch(err) {}
          }
      }
 
  </script></span><span id="j_id0:SupportTemplate:j_id177">
