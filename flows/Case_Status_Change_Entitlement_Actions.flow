    <decisions>
        <name>Open_Status</name>
        <label>Open Status?</label>
        <locationX>604</locationX>
        <locationY>554</locationY>
        <defaultConnector>
            <targetReference>Closed_Status</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes_Open</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Open</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Previous_Status_New</targetReference>
            </connector>
            <label>Yes Open</label>
        </rules>
    </decisions>
    <decisions>
        <description>Only the first move of New &gt; Open should count to complete the milestones.  Or if it pops straight to open bypassing new.</description>
        <name>Previous_Status_New</name>
        <label>Previous Status New?</label>
        <locationX>797</locationX>
        <locationY>557</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record__Prior.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>New</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record__Prior.Status</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Uncompleted_Response_Milestone_Only</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
