    <description>Updates the related Case Issue Weights when Opportunity or Account changes.</description>
    <formulas>
        <name>FormulaCalculateImportanceWeight</name>
        <dataType>Number</dataType>
        <expression>IF( AND({!CaseLoop.Record_Type__c}=&quot;Service&quot;, ISPICKVAL({!AccountRecord.Service_Account_Classification__c}, &quot;Marquee&quot;)), 10, 0
)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>FormulaCalculateOpptyWeight</name>
        <dataType>Number</dataType>
        <expression>IF ({!OpportunityValue} &lt; 500000, 0,

      IF (AND({!OpportunityValue} &gt;=500000, {!OpportunityValue} &lt;= 999999),3,

          IF ({!OpportunityValue} &gt;= 1000001,5,0
)
)
)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>GetUser</name>
        <dataType>String</dataType>
        <expression>{!$User.Full_Name__c}</expression>
    </formulas>
