<aura:component >
    <aura:attribute name="folderName" type="String" />
    
    <lightning:overlayLibrary aura:id="overlayLib"/>
    <lightning:button name="cancel" label="Cancel" onclick="{!c.handleCancel}" />
    <lightning:button name="ok" label="Create" variant="brand" onclick="{!c.handleCreateFolder}" disabled="{!v.folderName.length == 0}" />
</aura:component>